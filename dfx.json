{
    "version": 1,
    "canisters": {
        "staging": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            }
        },
        "copying-force": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            },
            "args": "--force-gc --copying-gc"
        },
        "compacting-force": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            },
            "args": "--force-gc --compacting-gc"
        },
        "generational-force": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            },
            "args": "--force-gc --generational-gc"
        },
        "copying": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            },
            "args": "--copying-gc"
        },
        "compacting": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            },
            "args": "--compacting-gc"
        },
        "generational": {
            "type": "motoko",
            "main": "src/main.mo",
            "declarations": {
                "output": "declarations/main"
            },
            "args": "--generational-gc"
        },
        "ledger": {
            "type": "custom",
            "wasm": "./declarations/ledger/ledger.wasm",
            "candid": "./declarations/ledger/ledger.private.did",
            "build": "",
            "remote": {
                "candid": "./declarations/ledger/ledger.public.did",
                "id": {
                    "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
                }
            }
        },
        "production": {
            "type": "motoko",
            "main": "src/main.mo"
        }
    },
    "defaults": {
        "build": {
            "packtool": "vessel sources",
            "args": "--trap-on-call-error"
        }
    }
}
